{
  "name": "@clutch-creator/toolkit",
  "version": "2.2.5",
  "description": "Toolkit components and utils used in clutch projects",
  "author": "Clutch Creator",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.14",
  "keywords": [
    "react",
    "NextJs",
    "clutch"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./src/components/*/index.tsx",
      "default": "./dist/components/*/index.js"
    },
    "./state": {
      "types": "./src/state/index.ts",
      "default": "./dist/state/index.js"
    },
    "./sitemap": {
      "types": "./src/sitemap/index.ts",
      "default": "./dist/sitemap/index.js"
    },
    "./server": {
      "types": "./src/server/index.ts",
      "default": "./dist/server/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./dist/utils/index.js"
    },
    "./next/*": {
      "types": "./src/next/*/index.ts",
      "default": "./dist/next/*"
    },
    "./ai": {
      "types": "./src/ai/index.ts",
      "default": "./dist/ai/index.js"
    }
  },
  "type": "module",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "swc src --out-dir dist --config-file ./.swcrc --delete-dir-on-start --strip-leading-paths",
    "dev": "bun run build --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "bun run build && changeset publish",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "ai": "5.0.81",
    "@ai-sdk/mcp": "0.0.3",
    "mcp-oauth-provider": "^1.0.0",
    "zustand": "^5.0.3",
    "qs-esm": "^7.0.2",
    "plaiceholder": "^3.0.0",
    "schema-dts": "^1.1.5"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.20.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.3.60",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.0.0",
    "@types/bun": "^1.2.22",
    "chokidar": "^4.0.3",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^27.0.0",
    "next": "^15.3.3",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "next": "^15.3.3"
  }
}
