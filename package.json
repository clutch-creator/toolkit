{
  "name": "@clutch-creator/toolkit",
  "version": "0.0.1",
  "description": "Toolkit components and utils used in clutch projects",
  "author": "Clutch Creator",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.14",
  "keywords": [
    "react",
    "NextJs",
    "clutch"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./components/*": "./dist/components/*/index.js",
    "./state": "./dist/state/index.js",
    "./sitemap": "./dist/sitemap/index.js",
    "./server": "./dist/server/index.js",
    "./utils": "./dist/utils/index.js",
    "./next/*": "./dist/next/*"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:swc": "swc src --out-dir dist --config-file ./.swcrc --delete-dir-on-start --strip-leading-paths",
    "build:tsc": "tsc --emitDeclarationOnly --outDir dist",
    "build": "build:swc && build:tsc",
    "dev": "bun run build:swc --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "zustand": "^5.0.3",
    "qs-esm": "^7.0.2",
    "plaiceholder": "^3.0.0",
    "schema-dts": "^1.1.5"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.20.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.3.60",
    "@types/react": "^19.0.0",
    "chokidar": "^4.0.3",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "next": "^15.3.3",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "next": "^15.3.3"
  }
}
